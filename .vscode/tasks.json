{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "-I${fileDirname}",
                "${file}",
                "parser.c",
                "-o",
                "parser.h", 
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },

        {
            "type": "shell",
            "label": "gcc build",
            "command": "gcc",
            "options": {},
            "args": [
                "-g",
                "${workspaceFolder}/mmn14/assembler.c",
                "${workspaceFolder}/mmn14/hashtable.c",
                "${workspaceFolder}/mmn14/encoder.c",
                "${workspaceFolder}/mmn14/parser.c",
                "${workspaceFolder}/mmn14/utilities.c",
                "${workspaceFolder}/mmn14/linkedlist.h",
                "${workspaceFolder}/mmn14/linkedlist.c",
                "${workspaceFolder}/mmn14/macro.c",
                "${workspaceFolder}/mmn14/errors.c",
                "-o",
                "${workspaceFolder}/mmn14/debug"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ],

    
    "version": "2.0.0"
}